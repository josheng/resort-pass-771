<% @listing = Listing.all %>
<% states = [] %>
<% @listing.each { |l| states << l.state unless states.include?(l.state) } %>
<div class="cards">
  <% states.first(6).each do |state| %>
    <%= link_to filter_listing_path(state) do  %>
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://source.unsplash.com/1920x1080/?<%= state.gsub(" ","-") %>,resort)">
        <small>Explore</small>
        <%=  state %>
      </div>
  <% end %>
  <% end %>
</div>
